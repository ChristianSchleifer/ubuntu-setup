[CPU]
full_text= CPU:  0.00% @ +00.0°C 
command=~/.config/i3/i3blocks/cpu/cpu_info.sh
interval=repeat
color=#00B4EB

[BATTERY]
command=~/.config/i3/i3blocks/battery/battery_info.sh
interval=3

[MEM]
command=free -h | awk '/Mem:/ { printf(" 🐏 %5s/%s \n", $3, $2) }'
interval=1
color=#FEC925

[TIME_DATE]
command=date +" %a, %d %b - %H:%M:%S"
interval=1
